        <clear />
        <add name="UmbracoRoleProvider" type="Umbraco.Web.Security.Providers.MembersRoleProvider" />
      </providers>
    </roleManager>
    <machineKey validationKey="ADF597D49728304C557D1F8DF5479C325A5867270A20BB3927BE06D62EAA7ABC" decryptionKey="F70D8D29E6FAECA9485B74EAD04A253D8DE13830B1B3E75B209E254590D88810" validation="HMACSHA256" decryption="AES" />
  </system.web>
